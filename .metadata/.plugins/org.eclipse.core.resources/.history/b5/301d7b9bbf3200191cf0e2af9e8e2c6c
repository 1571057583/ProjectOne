class Parent{
 private int x;
 protected int y;
 public Parent(int x, int y) {
this.x = x;
this.y = y;
}
 public void increaseX(int x){
 this.x = getX()+x;
 }
public int getX(){
 return x;
 }
 public void increaseY(int y){
 this.y = getY()+y;
 }
 public int getY(){
 return y;
}
}
class Child extends Parent
{
private int x;
 private int y;
 public Child(int x, int y){
 super(x, y);
this.y = y+250;
this.x = x+150;
}
 public int getX(){
 return x;
 }
 public int getY(){
 return y;
} 
}




